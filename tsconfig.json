    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es2018",
    "experimentalDecorators": true,
  },
  "lib": ["es2018"],
}
